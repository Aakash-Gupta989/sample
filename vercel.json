{
  "version": 2,
  "buildCommand": "cd frontend && npm install && CI=false npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "cd frontend && npm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://prepandhirebackend.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}